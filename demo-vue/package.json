{
  "nativescript": {
    "id": "org.nativescript.picker.demovue",
    "tns-android": {
      "version": "6.0.0"
    },
    "tns-ios": {
      "version": "6.0.1"
    }
  },
  "description": "NativeScript PickerField Demo Vue",
  "scripts": {
    "run.ios": "tns run ios --provision NativeScriptDevProfile",
    "run.android": "tns run android --provision NativeScriptDevProfile",
    "build.plugin": "cd ../src && npm run build",
    "ci.tslint": "npm i && tslint --config '../tslint.json' 'src/**/*.ts'",
  },
  "dependencies": {
    "nativescript-picker": "file:../src",
    "nativescript-theme-core": "~1.0.4",
    "nativescript-vue": "~2.3.0",
    "tns-core-modules": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "~7.2.0",
    "@babel/preset-env": "~7.2.0",
    "@types/node": "~10.12.18",
    "babel-loader": "~8.0.0",
    "nativescript-dev-webpack": "~1.0.1",
    "nativescript-vue-template-compiler": "~2.3.0",
    "node-sass": "~4.12.0",
    "vue-loader": "~15.4.0",
    "typescript": "~3.4.5"
  },
  "readme": "NativeScript PickerField Demo Vue"
}
